<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainframe" prefHeight="489.0" prefWidth="538.0"
            style="-fx-background-color: BLACK;" stylesheets="@css/main.css" xmlns="http://javafx.com/javafx/18"
            fx:controller="gok.simplepdf_GUI.mainUsingListView_Controller">
   <children>
      <Text fill="WHITE" layoutX="14.0" layoutY="24.0" lineSpacing="5.0" stroke="BLACK" strokeLineCap="ROUND"
            strokeLineJoin="BEVEL" strokeType="OUTSIDE" strokeWidth="0.0" text="PDF Merger" textAlignment="CENTER"
            wrappingWidth="128.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="396.0"
            AnchorPane.topAnchor="-1.2197265625">
         <font>
            <Font name="Impact" size="25.0"/>
         </font>
      </Text>
      <FontAwesomeIcon fx:id="exitBtn" accessibleRole="BUTTON" fill="RED" glyphName="TIMES" layoutX="515.0"
                       layoutY="23.0" onMouseClicked="#exit" size="2em"/>
      <ListView fx:id="myListView" editable="true" layoutY="36.0" prefHeight="332.0" prefWidth="538.0"
                style="-fx-background-color: BLACK;" AnchorPane.bottomAnchor="127.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0"/>
      <Button fx:id="mergeFilesBtn" layoutX="396.0" layoutY="435.0" mnemonicParsing="false" onAction="#mergeFiles"
              prefHeight="45.0" prefWidth="128.0" text="Merge" AnchorPane.rightAnchor="14.0">
         <graphic>
            <FontAwesomeIcon fill="WHITE" glyphName="CHAIN" size="1.5em"/>
         </graphic>
         <font>
            <Font size="14.0"/>
         </font>
         <styleClass>
            <String fx:value="btn-info"/>
            <String fx:value="h4"/>
         </styleClass>
      </Button>
      <Separator layoutY="421.0" prefHeight="14.0" prefWidth="517.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <HBox layoutY="370.0" prefHeight="31.0" prefWidth="506.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="addFileBtn" mnemonicParsing="false" onAction="#getFiles" prefHeight="28.0" prefWidth="99.0"
                    styleClass="btn-success" text="Add File">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
               </padding>
               <graphic>
                  <FontAwesomeIcon fill="WHITE" glyphName="PLUS" size="1.5em" wrappingWidth="16.142857551574707"/>
               </graphic>
               <font>
                  <Font size="14.0"/>
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="deleteFileBtn" cancelButton="true" mnemonicParsing="false" onAction="#deleteFile"
                    prefHeight="28.0" prefWidth="99.0" styleClass="btn-danger" text="Delete">
               <graphic>
                  <FontAwesomeIcon fx:id="deleteFile" fill="WHITE" glyphName="TRASH" size="1.5em"
                                   wrappingWidth="14.428571701049805"/>
               </graphic>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
               </padding>
               <font>
                  <Font size="14.0"/>
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="moveUpBtn" cancelButton="true" mnemonicParsing="false" onAction="#moveUp" prefHeight="31.0"
                    prefWidth="115.0" styleClass="btn-warning" text="Move Up">
               <graphic>
                  <FontAwesomeIcon fill="WHITE" glyphName="REPLY" rotate="90.0" size="1.5em"/>
               </graphic>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
               </padding>
               <font>
                  <Font size="14.0"/>
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="moveDownBtn" cancelButton="true" mnemonicParsing="false" onAction="#moveDown"
                    prefHeight="35.0" prefWidth="128.0" styleClass="btn-warning" text="Move Down">
               <graphic>
                  <FontAwesomeIcon fill="WHITE" glyphName="REPLY" rotate="-90.0" size="1.5em"/>
               </graphic>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
               </padding>
               <font>
                  <Font size="14.0"/>
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
